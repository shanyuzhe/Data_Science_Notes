        },
        "outputId": "1f6f7ea5-8066-4491-b50f-cc72d691c5a5"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "a 的名字: 原始数组A\n",
            "b 的名字: 原始数组A\n",
            "c 的名字: no name\n"
          ]
        }
      ],
      "source": [
        "import numpy as np\n",
        "\n",
        "class NamedArray(np.ndarray):\n",
        "    # 1. __new__: 负责“生孩子”\n",
        "    # 当你显式调用 NamedArray([1,2], name=\"A\") 时触发\n",
        "    def __new__(cls, array, name=\"no name\"):\n",
        "        # 将输入的普通数据(array)转换成标准数组，并强行转换(view)成当前类(cls)\n",
        "        obj = np.asarray(array).view(cls)\n",
        "\n",
        "        # 给新诞生的对象赋予属性\n",
        "        obj.name = name\n",
        "\n",
        "        # 返回这个造好的对象\n",
        "        return obj\n",
        "\n",
        "    # 2. __array_finalize__: 负责“遗传基因”\n",
        "    # 当数组被切片、运算、或者做视图转换(view)时触发\n",
        "    def __array_finalize__(self, obj):\n",
        "        # 情况A: 如果是显式调用 __new__ 创建的，obj 是 None。\n",
        "        # 因为 __new__ 里已经处理好属性了，这里直接跳过。\n",
        "        if obj is None: return\n",
        "\n",
        "        # 情况B: 如果是通过切片(如 b = a[:])创建的。\n",
        "        # 此时 self 是新数组 b，obj 是老数组 a。\n",
        "        # 我们要从老数组(obj)里把 'name' 属性抄过来给新数组(self)。\n",
        "        # getattr(对象, '属性名', '默认值'): 防止 obj 只是个普通数组没有 name 属性\n",
        "        self.name = getattr(obj, 'name', \"no name\")\n",
        "\n",
        "# --- 测试代码 ---\n",
        "\n",
        "# 1. 显式创建（触发 __new__）\n",
        "a = NamedArray(np.arange(5), name=\"原始数组A\")\n",
        "print(f\"a 的名字: {a.name}\")\n",
        "# 输出: a 的名字: 原始数组A\n",
        "\n",
        "# 2. 切片创建（触发 __array_finalize__）\n",
        "# 注意：这里没有调用 NamedArray(...)，而是直接切片\n",
        "b = a[1:3]\n",
        "print(f\"b 的名字: {b.name}\")\n",
        "# 输出: b 的名字: 原始数组A (成功遗传了！)\n",
        "\n",
        "# 3. 视图转换（触发 __array_finalize__）\n",
        "# 把一个普通数组看作 NamedArray\n",
        "c = np.array([1, 2]).view(NamedArray)\n",
        "print(f\"c 的名字: {c.name}\")\n",
        "# 输出: c 的名字: no name (因为原数组没有name，使用了默认值)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "class MobilePhone:\n",
        "    # 1. 这里的 cls 代表 MobilePhone 这个类本身（工厂）\n",
        "    def __new__(cls, color):\n",
        "        print(f\"步骤1 [__new__]: 工厂 ({cls.__name__}) 正在制造手机硬件...\")\n",
        "\n",
        "        # 真正创建对象的一步\n",
        "        instance = super().__new__(cls)\n",
        "\n",
        "        print(f\"  -> 手机硬件造好了！内存地址是: {id(instance)}\")\n",
        "        # 必须把造好的手机返回去，才能进下一步\n",
        "        return instance\n",
        "\n",
        "    # 2. 这里的 self 代表刚才造好的那台手机\n",
        "    def __init__(self, color):\n",
        "        print(f\"步骤2 [__init__]: 用户拿到手机 ({id(self)})，正在给外壳喷漆...\")\n",
        "        self.color = color\n",
        "        print(f\"  -> 喷漆完成！颜色是 {self.color}\")\n",
        "\n",
        "# 开始购买\n",
        "print(\"--- 准备下单 ---\")\n",
        "my_phone = MobilePhone(\"黑色\")\n",
        "print(\"--- 购买结束 ---\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "8nJihaJT099T",
        "outputId": "3ff0d612-7f8c-481c-82b1-74d5cf2c6a9b"
      },
